<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- More Property Types -->
    <record id="property_type_apartment" model="estate.property.type">
        <field name="name">Apartment</field>
    </record>
    <record id="property_type_house" model="estate.property.type">
        <field name="name">House</field>
    </record>
    <record id="property_type_land" model="estate.property.type">
        <field name="name">Land</field>
    </record>

    <!-- More Property Tags -->
    <record id="property_tag_sea_view" model="estate.property.tag">
        <field name="name">Sea View</field>
        <field name="color">3</field>
    </record>
    <record id="property_tag_garden" model="estate.property.tag">
        <field name="name">Garden</field>
        <field name="color">5</field>
    </record>

    <!-- Properties -->
    <record id="property_krish_cubical5" model="estate.property">
