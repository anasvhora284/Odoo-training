        <field name="living_area">1500</field>
        <field name="expected_price">10000.00</field>
        <field name="selling_price">14000.00</field>
        <field name="property_type_id" ref="property_type_residential"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_1'), ref('property_tag_2')])]"/>
