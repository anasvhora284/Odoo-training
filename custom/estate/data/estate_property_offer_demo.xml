<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- More Properties -->
    <record id="property_3" model="estate.property">
        <field name="name">Luxury Apartment</field>
        <field name="state">new</field>
        <field name="description">Stunning penthouse with panoramic views</field>
        <field name="postcode">98765</field>
        <field name="date_availability" eval="DateTime.now()"/>
        <field name="expected_price">750000.00</field>
        <field name="bedrooms">3</field>
        <field name="living_area">180</field>
        <field name="property_type_id" ref="property_type_apartment"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_sea_view')])]"/>
        <field name="seller_id" ref="base.user_admin"/>
    </record>

    <record id="property_4" model="estate.property">
        <field name="name">Family House</field>
        <field name="state">new</field>
        <field name="description">Perfect family home with large garden</field>
        <field name="postcode">34567</field>
        <field name="date_availability" eval="DateTime.now()"/>
        <field name="expected_price">420000.00</field>
        <field name="bedrooms">5</field>
        <field name="living_area">250</field>
        <field name="garden">True</field>
        <field name="garden_area">200</field>
        <field name="garden_orientation">south</field>
        <field name="property_type_id" ref="property_type_house"/>
        <field name="tag_ids" eval="[(6, 0, [ref('property_tag_garden')])]"/>
        <field name="seller_id" ref="base.user_admin"/>
    </record>

    <!-- More Property Offers -->
    <record id="property_offer_3" model="estate.property.offer">
        <field name="price">700000.00</field>
        <field name="partner_id" ref="base.res_partner_4"/>
        <field name="property_id" ref="property_3"/>
        <field name="validity">10</field>
    </record>

    <record id="property_offer_4" model="estate.property.offer">
        <field name="price">400000.00</field>
        <field name="partner_id" ref="base.res_partner_1"/>
        <field name="property_id" ref="property_4"/>
        <field name="validity">20</field>
    </record>

    <!-- Property Offers -->
    <record id="offer_1" model="estate.property.offer">
        <field name="price">13500.00</field>
        <field name="partner_id" ref="base.res_partner_2"/>
        <field name="property_id" ref="property_krish_cubical5"/>
        <field name="validity">7</field>
    </record>

    <record id="offer_2" model="estate.property.offer">
        <field name="price">1900.00</field>
        <field name="partner_id" ref="base.res_partner_3"/>
        <field name="property_id" ref="property_shilp_epitome1"/>
