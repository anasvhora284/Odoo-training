<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Properties for Company 1 -->
        <record id="property_1" model="estate.property">
            <field name="name">Modern Villa</field>
            <field name="state">new</field>
            <field name="description">Beautiful modern villa with garden</field>
            <field name="postcode">12345</field>
            <field name="date_availability" eval="DateTime.now()"/>
            <field name="expected_price">500000.00</field>
            <field name="bedrooms">4</field>
            <field name="living_area">200</field>
            <field name="garden">true</field>
            <field name="garden_area">100</field>
            <field name="garden_orientation">north</field>
            <field name="property_type_id" ref="property_type_villa"/>
            <field name="tag_ids" eval="[(6, 0, [ref('property_tag_luxury'), ref('property_tag_garden')])]"/>
            <field name="seller_id" ref="base.user_admin"/>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="property_2" model="estate.property">
            <field name="name">City Apartment</field>
            <field name="state">new</field>
            <field name="description">Centrally located apartment</field>
            <field name="postcode">54321</field>
            <field name="date_availability" eval="DateTime.now()"/>
            <field name="expected_price">300000.00</field>
            <field name="bedrooms">2</field>
            <field name="living_area">80</field>
            <field name="property_type_id" ref="property_type_apartment"/>
            <field name="tag_ids" eval="[(6, 0, [ref('property_tag_affordable'), ref('property_tag_city_center')])]"/>
            <field name="seller_id" ref="base.user_demo"/>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="property_3" model="estate.property">
            <field name="name">Luxury Penthouse</field>
            <field name="state">new</field>
            <field name="description">Stunning penthouse with panoramic views</field>
            <field name="postcode">98765</field>
            <field name="date_availability" eval="DateTime.now()"/>
            <field name="expected_price">750000.00</field>
            <field name="bedrooms">3</field>
            <field name="living_area">180</field>
            <field name="property_type_id" ref="property_type_penthouse"/>
            <field name="tag_ids" eval="[(6, 0, [ref('property_tag_luxury'), ref('property_tag_sea_view')])]"/>
            <field name="seller_id" ref="base.user_admin"/>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="property_4" model="estate.property">
            <field name="name">Family House</field>
            <field name="state">new</field>
            <field name="description">Perfect family home with large garden</field>
            <field name="postcode">34567</field>
            <field name="date_availability" eval="DateTime.now()"/>
            <field name="expected_price">420000.00</field>
            <field name="bedrooms">5</field>
            <field name="living_area">250</field>
            <field name="garden">True</field>
            <field name="garden_area">200</field>
            <field name="garden_orientation">south</field>
            <field name="property_type_id" ref="property_type_house"/>
            <field name="tag_ids" eval="[(6, 0, [ref('property_tag_garden')])]"/>
            <field name="seller_id" ref="base.user_admin"/>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <!-- Properties for Company 2 -->
        <record id="property_5" model="estate.property">
            <field name="name">Shilp Epitome Tower A</field>
            <field name="state">new</field>
            <field name="description">Luxurious apartment with modern amenities in Tower A</field>
            <field name="postcode">67890</field>
            <field name="date_availability" eval="DateTime.now()"/>
            <field name="expected_price">850000.00</field>
            <field name="bedrooms">3</field>
            <field name="living_area">150</field>
            <field name="property_type_id" ref="property_type_apartment"/>
            <field name="tag_ids" eval="[(6, 0, [ref('property_tag_premium'), ref('property_tag_top_view')])]"/>
            <field name="seller_id" ref="base.user_admin"/>
            <field name="company_id" eval="ref('base.company_2') or ref('base.main_company')"/>
        </record>

        <record id="property_6" model="estate.property">
            <field name="name">Shilp Epitome Tower B</field>
            <field name="state">new</field>
            <field name="description">Compact luxury apartment in Tower B with city views</field>
            <field name="postcode">67890</field>
            <field name="date_availability" eval="DateTime.now()"/>
            <field name="expected_price">850000.00</field>
            <field name="bedrooms">2</field>
            <field name="living_area">120</field>
            <field name="property_type_id" ref="property_type_apartment"/>
            <field name="tag_ids" eval="[(6, 0, [ref('property_tag_premium'), ref('property_tag_city_center')])]"/>
            <field name="seller_id" ref="base.user_demo"/>
            <field name="company_id" eval="ref('base.company_2') or ref('base.main_company')"/>
        </record>

        <record id="property_7" model="estate.property">
            <field name="name">Shilp Epitome Penthouse</field>
            <field name="state">new</field>
            <field name="description">Exclusive penthouse with panoramic sea views</field>
            <field name="postcode">67890</field>
            <field name="date_availability" eval="DateTime.now()"/>
            <field name="expected_price">750000.00</field>
            <field name="bedrooms">4</field>
            <field name="living_area">200</field>
            <field name="property_type_id" ref="property_type_penthouse"/>
            <field name="tag_ids" eval="[(6, 0, [ref('property_tag_luxury'), ref('property_tag_sea_view'), ref('property_tag_top_view')])]"/>
            <field name="seller_id" ref="base.user_admin"/>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="property_8" model="estate.property">
            <field name="name">Shilp Epitome Villa</field>
            <field name="state">new</field>
            <field name="description">Luxurious villa with private garden in Epitome complex</field>
            <field name="postcode">67890</field>
            <field name="date_availability" eval="DateTime.now()"/>
            <field name="expected_price">950000.00</field>
            <field name="bedrooms">5</field>
            <field name="living_area">300</field>
            <field name="garden">True</field>
            <field name="garden_area">200</field>
            <field name="garden_orientation">south</field>
            <field name="property_type_id" ref="property_type_villa"/>
            <field name="tag_ids" eval="[(6, 0, [ref('property_tag_luxury'), ref('property_tag_garden'), ref('property_tag_premium')])]"/>
            <field name="seller_id" ref="base.user_admin"/>
            <field name="company_id" ref="base.main_company"/>
        </record>
    </data>
</odoo> 